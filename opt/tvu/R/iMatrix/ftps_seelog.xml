<seelog minlevel="debug">
	<outputs formatid="main">
		<buffered size="10000" flushperiod="1000">
			<rollingfile type="size" filename="log/libftps.log" maxsize="64000000" maxrolls="10"/>
		</buffered>
	</outputs>
	<formats>
		<format id="main" format="[%Date(2006-01-02 15:04:05.999 PM MST)] [%Level] [%File:%FuncShort#%Line] %Msg%n"/>
	</formats>
</seelog>